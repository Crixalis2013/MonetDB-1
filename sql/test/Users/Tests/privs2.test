statement ok
create function insertversion(iname varchar(10)
                             ,ii int
                             ) returns int
begin
  insert into version
    (name
    ,i
    )
  values
    (iname
    ,ii
    )
  ;
  return 1;
end

statement ok
create function updateversion(iname varchar(10)
                             ,ii int
                             ) returns int
begin
  update version
     set i = ii
    where name = iname
  ;
  return 1;
end

statement ok
create function deleteversion(iname varchar(10)
                             ) returns int
begin
  delete
    from version
   where name = iname
  ;
  return 1;
end

